/**app.wxss**/
@import 'style/weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
    background-color: #fff;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.evaluation-star {
  position: relative;
  display: inline-block;
  height: 21rpx;
  line-height: 21rpx;
}

.evaluation-star .star {
  display: inline-block;
  width: 21rpx; 
  height: 21rpx; 
  background: url(http://www.feelcss.com/weixin/star_s.png) no-repeat; 
  background-size: contain;
}

.evaluation-star.star-1 .star:nth-child(-n+1),
.evaluation-star.star-2 .star:nth-child(-n+2),
.evaluation-star.star-3 .star:nth-child(-n+3),
.evaluation-star.star-4 .star:nth-child(-n+4),
.evaluation-star.star-5 .star:nth-child(-n+5) { 
  background: url(http://www.feelcss.com/weixin/star_max.png) no-repeat; 
  background-size: contain; 
}